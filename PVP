    Title = "Unload Script",
    Description = "Tắt script và xóa UI",
    Callback = function()
        Window:Destroy()
        FOVCircle:Remove()
        for _, esp in pairs(ESP_Cache) do
            for _, d in pairs(esp) do d:Remove() end
        end
    end
})

-- ================= LOGIC LOOPS (VÒNG LẶP CHÍNH) =================

-- 1. RenderStepped: UI, ESP, Aimbot Camera
RunService.RenderStepped:Connect(function()
    -- Cập nhật FOV Circle theo chuột
    FOVCircle.Position = UserInputService:GetMouseLocation()
    FOVCircle.Radius = Config.FOVRadius
    FOVCircle.Visible = Config.ShowFOV

    -- Cập nhật ESP
    UpdateESP()

    -- Aimbot Logic
    if Config.Aimbot then
        local target = GetClosestPlayer()
        if target and target.Character and target.Character:FindFirstChild(Config.AimPart) then
            local targetPos = target.Character[Config.AimPart].Position
            local currentCF = Camera.CFrame
            local goalCF = CFrame.new(currentCF.Position, targetPos)
            
            -- Smooth Aim
            if Config.AimSmooth <= 0.05 then
                Camera.CFrame = goalCF
            else
                Camera.CFrame = currentCF:Lerp(goalCF, 1 - Config.AimSmooth)
            end

            -- Rotate Character (Logic cũ)
            if Config.RotateCharacter and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                local hrp = LocalPlayer.Character.HumanoidRootPart
                local lookAt = Vector3.new(targetPos.X, hrp.Position.Y, targetPos.Z)
                hrp.CFrame = CFrame.new(hrp.Position, lookAt)
            end
            
            -- TriggerBot Logic
            if Config.TriggerBot then
                local mouseTarget = Mouse.Target
                if mouseTarget and mouseTarget:IsDescendantOf(target.Character) then
                    Attack()
                end
            end
        end
    end
end)

-- 2. Heartbeat: Physics, Movement, Combat
RunService.Heartbeat:Connect(function()
    local char = LocalPlayer.Character
    if not char or not char:FindFirstChild("HumanoidRootPart") then return end
    local hrp = char.HumanoidRootPart
    local hum = char:FindFirstChild("Humanoid")

    -- Hitbox Expander
    if Config.Hitbox then
        for _, plr in pairs(Players:GetPlayers()) do
            if IsValidTarget(plr) then
                local eHrp = plr.Character:FindFirstChild("HumanoidRootPart")
                if eHrp then
                    eHrp.Size = Vector3.new(Config.HitboxSize, Config.HitboxSize, Config.HitboxSize)
                    eHrp.Transparency = Config.HitboxTransparency
                    eHrp.CanCollide = false
                end
            end
        end
    else
        -- Reset Hitbox (Cơ bản)
        for _, plr in pairs(Players:GetPlayers()) do
            if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                local eHrp = plr.Character.HumanoidRootPart
                if eHrp.Size.X == Config.HitboxSize then
                    eHrp.Size = Vector3.new(2, 2, 1)
                    eHrp.Transparency = 1
                end
            end
        end
    end

    -- Kill Aura
    if Config.KillAura then
        for _, plr in pairs(Players:GetPlayers()) do
            if IsValidTarget(plr) then
                local eHrp = plr.Character:FindFirstChild("HumanoidRootPart")
                if eHrp and (hrp.Position - eHrp.Position).Magnitude <= Config.KillAuraRange then
                    -- Quay mặt về địch để đánh
                    hrp.CFrame = CFrame.new(hrp.Position, Vector3.new(eHrp.Position.X, hrp.Position.Y, eHrp.Position.Z))
                    Attack()
                end
            end
        end
    end

    -- Fly Logic
    if Config.Fly then
        local bv = hrp:FindFirstChild("DinoFlyVelocity") or Instance.new("BodyVelocity")
        bv.Name = "DinoFlyVelocity"
        bv.Parent = hrp
        bv.MaxForce = Vector3.new(1e5, 1e5, 1e5)
        
        local camCF = Camera.CFrame
        local vel = Vector3.new()
        
        if UserInputService:IsKeyDown(Enum.KeyCode.W) then vel = vel + camCF.LookVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.S) then vel = vel - camCF.LookVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.A) then vel = vel - camCF.RightVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.D) then vel = vel + camCF.RightVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.Space) then vel = vel + Vector3.new(0, 1, 0) end
        if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then vel = vel - Vector3.new(0, 1, 0) end
        
        bv.Velocity = vel * Config.FlySpeed
    else
        local bv = hrp:FindFirstChild("DinoFlyVelocity")
        if bv then bv:Destroy() end
    end

    -- Noclip
    if Config.Noclip then
        for _, part in pairs(char:GetDescendants()) do
            if part:IsA("BasePart") and part.CanCollide then part.CanCollide = false end
        end
    end

    -- Speed / Jump
    if hum then
        if Config.WalkSpeed > 16 then hum.WalkSpeed = Config.WalkSpeed end
        if Config.JumpPower > 50 then hum.JumpPower = Config.JumpPower end
    end
end)

-- 3. Event Handling (Xử lý sự kiện)

-- Thêm ESP khi người chơi mới vào
Players.PlayerAdded:Connect(function(plr)
    AddESP(plr)
    plr.CharacterAdded:Connect(function() AddESP(plr) end)
end)

-- Xóa ESP khi người chơi thoát
Players.PlayerRemoving:Connect(function(plr)
    RemoveESP(plr)
end)

-- Khởi tạo ESP cho người chơi đã có sẵn
for _, plr in pairs(Players:GetPlayers()) do
    if plr ~= LocalPlayer then
        AddESP(plr)
        plr.CharacterAdded:Connect(function() AddESP(plr) end)
    end
end

-- Infinite Jump
UserInputService.JumpRequest:Connect(function()
    if Config.InfJump and LocalPlayer.Character then
        local hum = LocalPlayer.Character:FindFirstChild("Humanoid")
        if hum then hum:ChangeState("Jumping") end
    end
end)

-- Anti-Kick (Hook Metamethod)
local OldNameCall = nil
pcall(function()
    if getnamecallmethod and hookmetamethod then
        OldNameCall = hookmetamethod(game, "__namecall", function(Self, ...)
            local Method = getnamecallmethod()
            if Config.AntiKick and Self == LocalPlayer and (Method == "Kick" or Method == "kick") then
                return nil
            end
            return OldNameCall(Self, ...)
        end)
    end
end)

-- Anti-AFK
LocalPlayer.Idled:Connect(function()
    if Config.AntiAFK then
        local vu = game:GetService("VirtualUser")
        vu:CaptureController()
        vu:ClickButton2(Vector2.new())
    end
end)

-- ================= HOÀN TẤT =================
SaveManager:SetLibrary(Fluent)
InterfaceManager:SetLibrary(Fluent)
SaveManager:IgnoreThemeSettings()
SaveManager:SetFolder("DinoPVP_V4")
InterfaceManager:BuildInterfaceSection(Tabs.Settings)

Window:SelectTab(1)

Fluent:Notify({
    Title = "DinoPVP ",
    Content = "Beta",
    Duration = 8
})
