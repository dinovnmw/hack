VisualsGroup:AddToggle("ESPTeam", {Title = "Team Check", Default = true, Callback = function(v) Config.ESPTeamCheck = v end})
-- [[ MOVEMENT TAB ]]
local MoveGroup = Tabs.Movement:AddSection("Character")
