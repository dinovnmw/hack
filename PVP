-- ================= CONFIGURATION =================
local Config = {
    -- Aimbot
    Aimbot = false,
    AimMode = "Nearest Character", -- Options: "Nearest Character", "Specific Player"
    SelectedTarget = "",
    AimPart = "Head",
    AimSmooth = 0.1, -- 0 = Instant Lock (Rage), 1 = Legit
    
    -- Combat
    KillAura = false,
    KillAuraRange = 20,
    TriggerBot = false,
    Hitbox = false,
    HitboxSize = 10,
    Spinbot = false,
    SpinSpeed = 50,
    
    -- Visuals
    ESP = false,
    ESPTeamCheck = true,
    
    -- Movement
    WalkSpeed = 16,
    JumpPower = 50,
    Fly = false,
    FlySpeed = 50,
    Noclip = false,
    InfJump = false
}

-- ================= UI SETUP =================
local Window = Fluent:CreateWindow({
    Title = "DinoPVP V2",
    SubTitle = "Ultimate PVP Suite",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true,
