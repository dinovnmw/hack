    Title = "Anti-Kick (Client)",
    Description = "Prevents local scripts from kicking you",
    Default = false,
    Callback = function(v)
        Config.AntiKick = v
        if v then Fluent:Notify({Title = "Security", Content = "Anti-Kick Active", Duration = 3}) end
